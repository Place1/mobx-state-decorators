{
  "name": "mobx-state-decorators",
  "version": "0.0.6",
  "description": "A utility library to make syncing mobx observables with other browser data sources easier, such as localstorage and the url bar!",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "test": "jest",
    "build": "npm run clean; tsc",
    "start": "webpack-dev-server -d --hot --inline --config ./example/webpack.config.js",
    "lint": "tslint --project ./ 'src/**/*.{ts,tsx}'",
    "clean": "rm -rf ./dist/ || true"
  },
  "author": "James Batt",
  "license": "MIT",
  "peerDependencies": {
    "reflect-metadata": "^0.1.12",
    "mobx": "^4.1.0"
  },
  "dependencies": {
    "query-string": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.0",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.0.41",
    "@types/react-dom": "^16.0.4",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.3",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reflect-metadata": "^0.1.12",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test_utils/jest_preprocessor.js"
    },
    "testRegex": "/*.test.(ts|tsx|js|jsx)$"
  }
}
