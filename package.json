{
  "name": "mobx-state-decorators",
  "version": "0.0.4",
  "description": "A utility library to make syncing mobx observables with other browser data sources easier, such as localstorage and the url bar!",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "test": "jest",
    "build": "npm run clean; tsc",
    "start": "webpack-dev-server -d --hot --inline --config ./example/webpack.config.js",
    "lint": "tslint --project ./ 'src/**/*.{ts,tsx}'",
    "clean": "rm -rf ./dist/ || true"
  },
  "author": "James Batt",
  "license": "MIT",
  "peerDependencies": {
    "reflect-metadata": "^0.1.10",
    "mobx": "^3.3.1"
  },
  "dependencies": {
    "query-string": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.6",
    "@types/node": "^8.0.20",
    "@types/query-string": "^5.0.0",
    "@types/react": "^16.0.13",
    "@types/react-dom": "^16.0.1",
    "awesome-typescript-loader": "^3.2.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "reflect-metadata": "^0.1.10",
    "source-map-loader": "^0.2.1",
    "tslint": "^5.6.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.1",
    "webpack-dev-server": "^2.7.1"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test_utils/jest_preprocessor.js"
    },
    "testRegex": "/*.test.(ts|tsx|js|jsx)$"
  }
}
